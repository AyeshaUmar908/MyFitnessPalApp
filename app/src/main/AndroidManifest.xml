<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myfitnesspal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HeightActivity"
            android:parentActivityName=".IntroActivity"
            />
        <activity
            android:name=".Goal"
            android:parentActivityName=".SignUp" />
        <activity
            android:name=".IntroActivity"
            android:parentActivityName=".Goal" />
        <activity android:name=".Help" />
        <activity android:name=".Settings" />
        <activity android:name=".Message" />
        <activity android:name=".Reminder" />
        <activity android:name=".Meal" />
        <activity android:name=".Nutrition" />
        <activity android:name=".Home" />
        <activity android:name=".Congrats" />
        <activity
            android:name=".ForgotPassword"
            android:parentActivityName=".LogIn" />
        <activity
            android:name=".EmailSignUp"
            android:parentActivityName=".SignUp" />
        <activity
            android:name=".LogIn"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SignUp"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>